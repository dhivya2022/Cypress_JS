{
    "defaultCommandTimeout":6000,
    "viewportHeight":760,
    "viewportWidth":1100,
    "env":{
        "application_URL":"https://rahulshettyacademy.com/angularpractice/",
        "application_URL_AutomationPractice":"https://rahulshettyacademy.com/AutomationPractice/"      
    },
    "projectId": "sahoze",
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
        "reportDir": "cypress/reports/mochawesome-report",
        "charts": true,
        "reportPageTitle": "Cypress-Mochawesome-Reporter",
        "embeddedScreenshots":true,
        "inlineAssets":true,
        "overwrite":false,
        "autoOpen":true,
        "reportFilename":"Automation_UI_Report_[datetime]",
        "code":true,
        "timestamp":"longDate",
        "showPassed":true
    },
    "retries": {
        "runMode": 1, 
        "openMode": 0
    },
    "video":false
    
}

